{
  "name": "asset",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auliainovasi/asset"
  },
  "author": "aulia rafi",
  "license": "MIT",
  "scripts": {
    "build-ts": "tsc",
    "build": "yarn run build-ts && yarn run lint",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start": "yarn run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run serve-debug\"",
    "watch-node": "nodemon src/server.ts",
    "watch-test": "yarn run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn run watch-ts\" \"yarn run watch-node\""
  },
  "dependencies": {
    "axios": "^1.3.1",
    "body-parser": "^1.19.1",
    "chromedriver": "^109.0.0",
    "compression": "^1.7.4",
    "csv-parse": "^5.3.4",
    "dotenv": "^16.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.2",
    "lusca": "^1.7.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "selenium-webdriver": "^4.8.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/chromedriver": "^81.0.1",
    "@types/compression": "^1.7.2",
    "@types/concurrently": "^6.4.0",
    "@types/csv-parse": "^1.2.2",
    "@types/dotenv": "^8.2.0",
    "@types/errorhandler": "^1.5.0",
    "@types/eslint": "^8.2.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/lusca": "^1.7.1",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.6",
    "@types/selenium-webdriver": "^4.1.10",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}